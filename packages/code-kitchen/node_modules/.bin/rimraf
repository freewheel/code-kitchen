#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/code-kitchen/code-kitchen/node_modules/.pnpm/rimraf@3.0.2/node_modules/rimraf/node_modules:/home/runner/work/code-kitchen/code-kitchen/node_modules/.pnpm/rimraf@3.0.2/node_modules:/home/runner/work/code-kitchen/code-kitchen/node_modules/.pnpm/node_modules:/home/runner/work/code-kitchen/code-kitchen/node_modules:/home/runner/work/code-kitchen/node_modules:/home/runner/work/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/work/code-kitchen/code-kitchen/packages/code-kitchen/node_modules:/home/runner/work/code-kitchen/code-kitchen/packages/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/runner/work/code-kitchen/code-kitchen/node_modules/.pnpm/rimraf@3.0.2/node_modules/rimraf/node_modules:/home/runner/work/code-kitchen/code-kitchen/node_modules/.pnpm/rimraf@3.0.2/node_modules:/home/runner/work/code-kitchen/code-kitchen/node_modules/.pnpm/node_modules:/home/runner/work/code-kitchen/code-kitchen/node_modules:/home/runner/work/code-kitchen/node_modules:/home/runner/work/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/work/code-kitchen/code-kitchen/packages/code-kitchen/node_modules:/home/runner/work/code-kitchen/code-kitchen/packages/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/rimraf@3.0.2/node_modules/rimraf/bin.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/rimraf@3.0.2/node_modules/rimraf/bin.js" "$@"
fi
